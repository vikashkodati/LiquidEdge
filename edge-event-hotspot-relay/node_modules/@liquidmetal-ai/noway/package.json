{
  "name": "@liquidmetal-ai/noway",
  "private": false,
  "version": "0.2.2",
  "description": "JWT wrapper with KV caching built on jose",
  "main": "dist/index.js",
  "type": "module",
  "dependencies": {
    "jose": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/chai": "^5",
    "@types/mocha": "^10",
    "@types/node": "^22",
    "@vitest/coverage-v8": "^2.1.1",
    "chai": "^5",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9",
    "eslint-plugin-only-warn": "^1.1.0",
    "globals": "^15.9.0",
    "mocha": "^10",
    "shx": "^0.3.4",
    "ts-node": "^10",
    "typescript": "^5",
    "typescript-eslint": "^8.7.0",
    "vitest": "^2.1.1",
    "vitest-fetch-mock": "^0.3.0",
    "@liquidmetal-ai/eslint-config": "0.0.0",
    "@liquidmetal-ai/typescript-config": "0.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist"
  ],
  "keywords": [],
  "author": "Ian Sung-Schenck",
  "license": "ISC",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "shx rm -rf dist",
    "prebuild": "pnpm clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "eslint . --max-warnings=0",
    "test": "vitest run",
    "test:watch": "vitest"
  }
}