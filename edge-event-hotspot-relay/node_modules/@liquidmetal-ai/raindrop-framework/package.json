{
  "name": "@liquidmetal-ai/raindrop-framework",
  "version": "0.9.2",
  "description": "Raindrop Cloud Framework",
  "main": "dist/index.js",
  "type": "module",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250607.0",
    "@eslint/js": "^9.11.1",
    "@types/node": "^22",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9",
    "json-schema-to-typescript": "^12.0.0",
    "shx": "^0.3.4",
    "typescript": "^5",
    "typescript-eslint": "^8.7.0",
    "vitest": "^2.1.1",
    "@liquidmetal-ai/typescript-config": "0.0.0",
    "@liquidmetal-ai/eslint-config": "0.0.0"
  },
  "dependencies": {
    "zod": "^3",
    "@liquidmetal-ai/noway": "0.2.2"
  },
  "author": "",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./testing": {
      "default": "./dist/testing/index.js"
    },
    "./core/*": {
      "import": "./dist/core/*.js",
      "types": "./dist/core/*.d.ts"
    }
  },
  "scripts": {
    "generate-version": "node scripts/generate-version.mjs",
    "build": "pnpm run generate-version && shx rm -rf dist && tsc -b",
    "lint": "eslint . --max-warnings=0",
    "test": "vitest run",
    "test:watch": "vitest"
  }
}